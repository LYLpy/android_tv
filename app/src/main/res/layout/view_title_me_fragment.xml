<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/view_titile_me_fragment_re_layout"
    android:layout_width="match_parent"
    android:layout_marginTop="@dimen/sw_25dp"
    android:layout_marginBottom="@dimen/sw_25dp"
    android:layout_height="wrap_content">
    <TextView
        android:layout_centerVertical="true"
        android:id="@+id/view_titile_me_fragment_text_name"
        android:text="个人信息"
        android:textColor="@color/color_me_fragment_text"
        android:textStyle="bold"
        android:textSize="@dimen/sw_30dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </TextView>
    <LinearLayout
        android:visibility="gone"
        android:layout_marginRight="@dimen/sw_15dp"
        style="@style/style_me_fragment_title_button_layout"
        android:padding="@dimen/sw_10dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:layout_toLeftOf="@+id/view_titile_me_fragment_but_return"
        android:id="@+id/view_titile_me_fragment_but_empty"
        android:layout_width="@dimen/sw_90dp"
        android:layout_height="@dimen/sw_40dp">
        <TextView
            android:textStyle="bold"
            android:textColor="@color/lb_action_text_color"
            android:text="清空"
            android:textSize="@dimen/sw_15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>



    <LinearLayout
        style="@style/style_me_fragment_title_button_layout"
        android:padding="@dimen/sw_10dp"
        android:gravity="center"
        android:orientation="horizontal"
        android:layout_alignParentRight="true"
        android:id="@+id/view_titile_me_fragment_but_return"
        android:layout_width="@dimen/sw_90dp"
        android:layout_height="@dimen/sw_40dp"
        android:layout_marginRight="@dimen/sw_12dp">
        <TextView
            android:textStyle="bold"
            android:textColor="@color/lb_action_text_color"
            android:text="返回"
            android:textSize="@dimen/sw_15dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
</RelativeLayout>